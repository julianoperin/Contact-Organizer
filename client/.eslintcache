op/Contact Organizer/client/src/context/contact/ContactState.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/context/contact/contactReducer.js",["108"],"import {\n  ADD_CONTACT,\n  DELETE_CONTACT,\n  SET_CURRENT,\n  CLEAR_CURRENT,\n  UPDATE_CONTACT,\n  FILTER_CONTACTS,\n  CLEAR_FILTER,\n} from \"../types\";\n\n//! State is in the CONTACTCONTEXT STATE\n//! ACTION comes with action.type & action. payload\nexport default (state, action) => {\n  switch (action.type) {\n    case ADD_CONTACT:\n      return {\n        ...state,\n        contacts: [...state.contacts, action.payload],\n      };\n    case UPDATE_CONTACT:\n      return {\n        ...state,\n        contacts: state.contacts.map((item) =>\n          item.id === action.payload.id ? action.payload : item\n        ),\n      };\n    case DELETE_CONTACT:\n      return {\n        ...state,\n        contacts: state.contacts.filter((item) => item.id !== action.payload),\n      };\n    case SET_CURRENT:\n      return {\n        ...state,\n        current: action.payload,\n      };\n    case CLEAR_CURRENT:\n      return {\n        ...state,\n        current: null,\n      };\n    case FILTER_CONTACTS:\n      return {\n        ...state,\n        filtered: state.contacts.filter((item) => {\n          const regex = new RegExp(`${action.payload}`, \"gi\");\n          return item.name.match(regex) || item.email.match(regex);\n        }),\n      };\n    case CLEAR_FILTER:\n      return {\n        ...state,\n        filtered: null,\n      };\n    default:\n      return state;\n  }\n};\n","/Users/julianoperin/Desktop/Contact Organizer/client/src/context/contact/contactContext.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/context/types.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/components/contacts/Contacts.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/components/contacts/ContactItem.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/components/pages/Budget.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/components/contacts/ContactForm.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/components/contacts/ContactFilter.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/context/auth/AuthState.js",["109","110","111"],"/Users/julianoperin/Desktop/Contact Organizer/client/src/context/auth/authContext.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/context/auth/authReducer.js",["112","113","114","115"],"/Users/julianoperin/Desktop/Contact Organizer/client/src/components/auth/Register.js",["116"],"import React, { useState, useContext, useEffect } from \"react\";\nimport AlertContext from \"../../context/alert/alertContext\";\nimport AuthContext from \"../../context/auth/authContext\";\n\nconst Register = () => {\n  const alertContext = useContext(AlertContext);\n  const authContext = useContext(AuthContext);\n\n  const { setAlert } = alertContext;\n\n  const { register, error, clearErrors } = authContext;\n\n  useEffect(() => {\n    if (error === \"User already exists\") {\n      setAlert(error, \"danger\");\n      clearErrors();\n    }\n  }, [error]);\n\n  const [user, setUser] = useState({\n    name: \"\",\n    email: \"\",\n    password: \"\",\n    password2: \"\",\n  });\n\n  const { name, email, password, password2 } = user;\n\n  const onChange = (e) => setUser({ ...user, [e.target.name]: e.target.value });\n\n  const onSubmit = (e) => {\n    e.preventDefault();\n\n    //! Front end validation\n    if (name === \"\" || email === \"\" || password === \"\") {\n      setAlert(\"Please enter all fields\", \"danger\");\n    } else if (password !== password2) {\n      setAlert(\"Passwords do not match\", \"danger\");\n    } else {\n      //! Send the data to the back end\n      register({\n        name,\n        email,\n        password,\n      });\n    }\n\n    console.log(\"Register submit\");\n  };\n\n  return (\n    <div className=\"form-container\">\n      <h1>\n        Account <span className=\"text-primary\">Register</span>\n      </h1>\n      <form onSubmit={onSubmit}>\n        <div className=\"form-group\">\n          <label htmlFor=\"name\">Name</label>\n          <input\n            type=\"text\"\n            name=\"name\"\n            value={name}\n            onChange={onChange}\n            required\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"email\">Email Address</label>\n          <input\n            type=\"email\"\n            name=\"email\"\n            value={email}\n            onChange={onChange}\n            required\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"password\">Password</label>\n          <input\n            type=\"password\"\n            name=\"password\"\n            value={password}\n            onChange={onChange}\n            minLength=\"6\"\n            required\n          />\n        </div>\n        <div className=\"form-group\">\n          <label htmlFor=\"password2\">Confirm Password</label>\n          <input\n            type=\"password\"\n            name=\"password2\"\n            value={password2}\n            onChange={onChange}\n            minLength=\"6\"\n            required\n          />\n        </div>\n        <input\n          type=\"submit\"\n          value=\"Register\"\n          className=\"btn btn-primary btn-block\"\n        />\n      </form>\n    </div>\n  );\n};\n\nexport default Register;\n","/Users/julianoperin/Desktop/Contact Organizer/client/src/components/auth/Login.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/context/alert/AlertState.js",[],"/Users/julianoperin/Desktop/Contact Organizer/client/src/context/alert/alertReducer.js",["117"],"import { SET_ALERT, REMOVE_ALERT } from \"../types\";\n\nexport default (state, action) => {\n  switch (action.type) {\n    case SET_ALERT:\n      return [...state, action.payload];\n  